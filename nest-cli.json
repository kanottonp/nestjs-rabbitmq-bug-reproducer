{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/inventory-1/src",
  "monorepo": true,
  "root": "apps/inventory-1/",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/inventory-1/tsconfig.app.json"
  },
  "projects": {
    "inventory-1": {
      "type": "application",
      "root": "apps/inventory-1",
      "entryFile": "main",
      "sourceRoot": "apps/inventory-1/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory-1/tsconfig.app.json"
      }
    },
    "inventory-2": {
      "type": "application",
      "root": "apps/inventory-2",
      "entryFile": "main",
      "sourceRoot": "apps/inventory-2/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory-2/tsconfig.app.json"
      }
    }
  }
}